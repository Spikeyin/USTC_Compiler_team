int main() {
    float result = 0.0;
    float x = 3.14159, y = 2.71828, z = 1.41421;  // 常用常量
    int i = 0;
    
    // 循环中包含大量重复的复杂计算
    while (i < 1000) {
        // 复杂表达式1：重复10次
        result = (x * y + z) / (x + y) * (z * z + x * y) + result;
        result = (x * y + z) / (x + y) * (z * z + x * y) + result;
        result = (x * y + z) / (x + y) * (z * z + x * y) + result;
        result = (x * y + z) / (x + y) * (z * z + x * y) + result;
        result = (x * y + z) / (x + y) * (z * z + x * y) + result;
        result = (x * y + z) / (x + y) * (z * z + x * y) + result;
        result = (x * y + z) / (x + y) * (z * z + x * y) + result;
        result = (x * y + z) / (x + y) * (z * z + x * y) + result;
        result = (x * y + z) / (x + y) * (z * z + x * y) + result;
        result = (x * y + z) / (x + y) * (z * z + x * y) + result;

        // 复杂表达式2：重复10次
        result = (z * x + y * y) / (z + x) * (y + x * z) + result;
        result = (z * x + y * y) / (z + x) * (y + x * z) + result;
        result = (z * x + y * y) / (z + x) * (y + x * z) + result;
        result = (z * x + y * y) / (z + x) * (y + x * z) + result;
        result = (z * x + y * y) / (z + x) * (y + x * z) + result;
        result = (z * x + y * y) / (z + x) * (y + x * z) + result;
        result = (z * x + y * y) / (z + x) * (y + x * z) + result;
        result = (z * x + y * y) / (z + x) * (y + x * z) + result;
        result = (z * x + y * y) / (z + x) * (y + x * z) + result;
        result = (z * x + y * y) / (z + x) * (y + x * z) + result;

        i = i + 1;
    }

    putfloat(result);
    return 0;
}